<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">
  <title>Let's Play Hub</title>
</head>
<body>

<div class="main-wrapper">

  <!-- section-menu start -->
  <div class="section-menu">
    <div class="logo">
      <a href="index.html">Let's Play Hub</a>
    </div>
    <div class="item">search</div>
    <div class="menu">
      <a href="games.html">
        <div class="item -choose">Games</div>
      </a>
      <a href="channels.html">
        <div class="item">Channels</div>
      </a>
    </div>
    <div class="action">
      <h3>Hello</h3>
      <p>You can here....</p>
      <p>And.....</p>
      <div class="sign">
        <div class="btn -in">Log in</div>
        <div class="btn -up">Sign up</div>
      </div>
    </div>
    <div class="footer">
      <div><a href="">About</a></div>
      <div class="language">
        <div class="en"><a href="">En</a></div>
        <div class="ru"><a href="">Ru</a></div>
      </div>
      <div class="version">
        2016 Â© LetsPlayHub.com, v.1.3.0
      </div>
    </div>
  </div>
  <!-- section-menu end -->

  <!-- section-content start -->
  <div class="section-content">
    <div class="wrapper">
      <div class="content">

        <!-- games-list start -->
        <div class="games-list">
          <div class="list-head">
            <div class="name"><h1>Games</h1></div>
            <div class="search">search for games</div>
          </div>
          <div class="list">
            <div class="title"><h3>TOP week</h3></div>
            <div class="wrapped-list -game">
              <% for(var i=0; i<10; i++) {%>
              <div class="holder">
                <div class="game-card">
                  <div class="preview"></div>
                </div>
              </div>
              <% } %>
              <% for(var i=0; i<10; i++) {%>
              <div class="placeholder"></div>
              <% } %>
            </div>
          </div>
          <div class="list">
            <div><h3>NEW games</h3></div>
            <div class="wrapped-list -game">
              <% for(var i=0; i<10; i++) {%>
              <div class="holder">
                <div class="game-card">
                  <div class="preview"></div>
                </div>
              </div>
              <% } %>
              <% for(var i=0; i<10; i++) {%>
              <div class="placeholder"></div>
              <% } %>
            </div>
          </div>

          <!-- featured-list start -->
          <div class="featured-list">
            <div class="wrapped-list -featured">
              <div class="holder">
                <div class="featured-card">
                  <div class="head"><h3>TOP Month</h3></div>
                  <div class="table">
                    <div class="title">
                      <div>Name game</div>
                      <div>Number videos</div>
                    </div>
                    <% for(var i=0; i<10; i++) {%>
                    <a href="">
                      <div class="row">
                        <div>game</div>
                        <div>1</div>
                      </div>
                    </a>
                    <% } %>
                  </div>
                </div>
              </div>
              <div class="holder">
                <div class="featured-card">
                  <div class="head"><h3>TOP Year</h3></div>
                  <div class="table">
                    <div class="title">
                      <div>Name game</div>
                      <div>Number videos</div>
                    </div>
                    <% for(var i=0; i<10; i++) {%>
                    <a href="">
                      <div class="row">
                        <div>game</div>
                        <div>1</div>
                      </div>
                    </a>
                    <% } %>
                  </div>
                </div>
              </div>
              <div class="holder">
                <div class="featured-card">
                  <div class="head"><h3>TOP All time</h3></div>
                  <div class="table">
                    <div class="title">
                      <div>Name game</div>
                      <div>Number videos</div>
                    </div>
                    <% for(var i=0; i<10; i++) {%>
                    <a href="">
                      <div class="row">
                        <div>game</div>
                        <div>1</div>
                      </div>
                    </a>
                    <% } %>
                  </div>
                </div>
              </div>
              <% for(var i=0; i<3; i++) {%>
              <div class="placeholder"></div>
              <% } %>
            </div>
          </div>
          <!-- featured-list end -->
        </div>
        <!-- games-list end -->
      </div>
    </div>
  </div>
  <!-- section-content end -->
</div>

</body>
</html>