<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">
  <title>Let's Play Hub</title>
</head>
<body>

<div class="main-wrapper">

  <!-- section-menu start -->
  <div class="section-menu">
    <div class="logo">
      <a href="index.html">Let's Play Hub</a>
    </div>
    <div class="item">search</div>
    <div class="menu">
      <a href="games.html">
        <div class="item">Games</div>
      </a>
      <a href="channels.html">
        <div class="item -choose">Channels</div>
      </a>
    </div>
    <div class="action">
      <h3>Hello</h3>
      <p>You can here....</p>
      <p>And.....</p>
      <div class="sign">
        <div class="btn -in">Log in</div>
        <div class="btn -up">Sign up</div>
      </div>
    </div>
    <div class="footer">
      <div><a href="">About</a></div>
      <div class="language">
        <div class="en"><a href="">En</a></div>
        <div class="ru"><a href="">Ru</a></div>
      </div>
      <div class="version">
        2016 Â© LetsPlayHub.com, v.1.3.0
      </div>
    </div>
  </div>
  <!-- section-menu end -->

  <!-- section-content start -->
  <div class="section-content">
    <div class="wrapper">

      <div class="content">

        <!-- channels-list start -->
        <div class="channels-list">
          <div class="list-head">
            <div class="name"><h1>Channels</h1></div>
            <div class="search">search for channels</div>
          </div>
          <div class="wrapped-list">
            <% for(var i=0; i<16; i++) {%>
            <div class="holder">
              <div class="channel-card">
                <div class="ava">
                  <div class="image"></div>
                </div>
                <div class="name"><h4>Name channel</h4></div>
                <div class="btn -subscribe">Subscribe</div>
                  <div class="video-card -channel">
                    <div class="preview"></div>
                  </div>
              </div>
            </div>
            <% } %>
            <% for(var i=0; i<8; i++) {%>
            <div class="placeholder"></div>
            <% } %>
          </div>
          <div class="next-page"><a href="">Next &rarr;</a></div>
        </div>
        <!-- channels-list end -->
      </div>
    </div>
  </div>
  <!-- section-content end -->
</div>

</body>
</html>